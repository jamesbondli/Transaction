<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.transaction">

    <application
        android:name=".model.CurrentUser"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Transaction">
        <activity android:name=".view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.TableActivity" />
        <activity android:name=".view.activity.ShopSearchActivity" />
        <activity android:name=".view.activity.ShopItemActivity" />
        <activity android:name=".view.activity.login.LoginActivity" />
        <activity android:name=".view.activity.login.ForgotActivity" />
        <activity android:name=".view.activity.login.RegisterActivity" />
        <activity android:name=".view.activity.SetPrivateActivity" />
    </application>

</manifest>